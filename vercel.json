{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "front-end/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "front-end/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}